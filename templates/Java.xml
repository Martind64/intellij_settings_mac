<templateSet group="Java">
  <template name="bra" value="() {&#10;}" description="method start" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="rnn" value="java.util.Objects.requireNonNull($VAR$, &quot;$VAR$ is required&quot;);" description="Require non null" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>